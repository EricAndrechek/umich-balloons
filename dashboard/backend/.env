# .env file

# PostgreSQL / PostGIS
POSTGRES_DB=gisdb
POSTGRES_USERNAME=gisuser
POSTGRES_PASSWORD=a_very_secure_password # CHANGE THIS!
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_QUEUE_DB=0
REDIS_CACHE_DB=1

# MQTT Pi Username and Password
MQTT_USERNAME=pi_user
MQTT_PASSWORD=a_secure_pi_password

# MQTT Bridge Username and Password
MQTT_BRIDGE_USERNAME=bridge_user
MQTT_BRIDGE_PASSWORD=a_secure_bridge_password
